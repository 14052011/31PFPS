FUNCTION_BLOCK "Avg01_code"
    VAR_INPUT
        X:    REAL;
        t:    REAL;
        init:    BOOL;
    END_VAR
    VAR_OUTPUT
        Q:    REAL;
    END_VAR
    VAR
        Q_s:    REAL;
        t_s:    REAL;
    END_VAR
BEGIN
    IF #init
    THEN #Q_s := #X; END_IF;
    #t_s := #t;
    IF #t < 0.1 THEN
        #t_s := 0.1;
    END_IF;
    #Q_s := #Q_s + ((#X - #Q_s) / #t / 10);
    #Q := #Q_s;
END_FUNCTION_BLOCK
