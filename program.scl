FUNCTION_BLOCK "KT19_code"
    VAR_INPUT
        xReset:    BOOL;
        T:    TIME;
    END_VAR
    VAR_OUTPUT
        xEnd:    BOOL;
        E:    TIME;
        L:    TIME;
    END_VAR
    VAR
        step:    TIME := T#100ms;
        xEnd_s:    BOOL;
        E_s:    TIME;
    END_VAR
BEGIN
    IF #xReset THEN
        #E_s    := T#0s;
        #xEnd_s    := 0;
    END_IF;
    IF #xEnd_s THEN #xEnd_s := 0; END_IF;
    IF dtclk.Q THEN
        #E_s := #E_s + #step;
        IF #E_s >= #T THEN
            #xEnd_s    := 1;
            #E_s    := T#0s;
        END_IF;
    END_IF;
    #xEnd    := #xEnd_s;
    #E    := #E_s;
    #L    := #T - #E_s;
END_FUNCTION_BLOCK
