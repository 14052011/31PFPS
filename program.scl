FUNCTION_BLOCK "Reg35P_code"
    VAR_INPUT
        A:    REAL;    
        SP:    REAL;    
        enable:    BOOL;
        off_value:    REAL;    
        mode:    INT;    
        kp:    REAL;    
        k3_P1:    REAL;
        k3_P2:    REAL;
        E_P1:    REAL;
        E_P2:    REAL;
        t5:    REAL;    
        k5_max:    REAL;    
        CZQ_k5low:    REAL;    
        CZQ_k5high:    REAL;    
        CZ_t:    REAL;    
        k13_max:    REAL;    
        CZQ_k13low:    REAL;    
        CZQ_k13high:    REAL;    
        QFC_up:    REAL;    
        QFC_down:    REAL;    
    END_VAR
    VAR_OUTPUT
        Q:    REAL;    
    END_VAR
    VAR
        avgA:    REAL;
        E:    REAL;
        E5:    REAL;
        k3:    REAL;
        k5:    REAL;
        k13:    REAL;
        Q3:    REAL;
        CZ_above1below0:    BOOL;
        CZQ:    REAL;
        CZQ_s:    REAL;
        CZ_cnt:    REAL;
        CZ_cnt_prev:    REAL;
        Q_full:    REAL;
        Q_full_prev:    REAL;
        Q_prevdir_up:    BOOL;
    END_VAR
BEGIN
    (* Copyright (c) 2019 Leonid Titov. All rights reserved *)
    (* www.avtprom.com *)
    IF dtclk.Q THEN
        IF (#E5 < 0 AND CZ_above1below0) OR (#E5 > 0 AND NOT CZ_above1below0) THEN
            #CZQ_s    := #CZ_cnt;
            #CZ_cnt_prev    := #CZ_cnt;
            #CZ_cnt    := 0;
        END_IF;
        #CZ_cnt := #CZ_cnt + 1;
        IF #CZ_cnt > 100000 THEN #CZ_cnt := 100000.0; END_IF;
        IF #CZ_cnt > #CZ_cnt_prev THEN
            #CZQ_s    := #CZ_cnt;
        END_IF;
        IF #E5 < 0 THEN CZ_above1below0 := 0; END_IF;
        IF #E5 > 0 THEN CZ_above1below0 := 1; END_IF;
        #CZQ := #CZQ + ((#CZQ_s - #CZQ) / CZ_t);
        #k5 := MapARange(
            v := #CZQ,
            i1:=#CZQ_k5low,    i2:=#CZQ_k5high,
            o1:=0.0,    o2:=#k5_max
        );
        #k5 := LimRange(L1:=0.0, L2:=#k5_max, v:=#k5);
        #k13 := MapARange(
            v := #CZQ,
            i1:=#CZQ_k13low,    i2:=#CZQ_k13high,
            o1:=0,    o2:=#k13_max
        );
        #k13 := LimRange(L1:=0, L2:=#k13_max, v:=#k13);
        #avgA := #avgA + ((#A - #avgA) / #t5);
        #E  := #A - #SP;
        #E5 := #E + ((#A - #avgA) * #k5);
        IF ABS(#E) < #E_P1 THEN
            #k3 := ABS(MapARange(
                v := #E,
                i1:=0.0,    i2:=#E_P1,
                o1:=0.0,    o2:=#k3_P1
            ));
        ELSE
            #k3 := ABS(MapARange(
                v := #E,
                i1:=#E_P1,    i2:=#E_P2,
                o1:=#k3_P1,    o2:=#k3_P2
            ));
        END_IF;
        IF #mode = 1 THEN
            #Q3    := #Q3 - (#E5 * #k3) - (#E * #k13);
            #Q_full    := #Q3 - (#E * #kp);
        ELSE
            #Q3    := #Q3 + (#E5 * #k3) + (#E * #k13);
            #Q_full    := #Q3 + (#E * #kp);
        END_IF;
        IF NOT enable THEN
            #Q3    := #off_value;
            #Q_full    := #off_value;
        END_IF;
        #Q3 := LimRange(L1:=0.0, L2:=100.0, v:=#Q3);
        IF #Q_full < #Q_full_prev AND #Q_prevdir_up THEN
            Q_full := #Q_full - QFC_down;
        ELSIF #Q_full > #Q_full_prev AND NOT #Q_prevdir_up THEN
            Q_full := #Q_full + QFC_up;
        END_IF;
        IF #Q_full < #Q_full_prev THEN
            #Q_prevdir_up := 0;
        ELSIF #Q_full > #Q_full_prev THEN
            #Q_prevdir_up := 1;
        END_IF;
        Q_full_prev := Q_full;
        #Q_full := LimRange(L1:=0.0, L2:=100.0, v:=#Q_full);
        #Q := #Q_full;
    END_IF;
END_FUNCTION_BLOCK
