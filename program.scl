ORGANIZATION_BLOCK "Main"
   VAR_TEMP 
      OB1_EV_CLASS : Byte;   
      OB1_SCAN_1 : Byte;   
      OB1_PRIORITY : Byte;   
      OB1_OB_NUMBR : Byte;   
      OB1_RESERVED_1 : Byte;   
      OB1_RESERVED_2 : Byte;   
      OB1_PREV_CYCLE : Int;   
      OB1_MIN_CYCLE : Int;   
      OB1_MAX_CYCLE : Int;   
      OB1_DATE_TIME : Date_And_Time;   
    r1:    REAL;
   END_VAR
BEGIN
    (* Copyright (c) 2019 Leonid Titov. All rights reserved. *)
    (* www.avtprom.com *)
    t1.TON(IN := NOT BD.x1,
                         PT := T#2s,
                         Q => BD.x1);
    t2.TOF(IN := t1.Q,
                           PT := T#1s,
                           Q => BD.x1);
    dtclk.TON(PT:=T#100ms, IN:= NOT dtclk.Q);   (* impulse generator idiom *)
    ep_GA1_btn(CLK := EnterButton1);
    ep_GA2_btn(CLK := EnterButton2);
    ep_GA3_btn(CLK := EnterButton3);
	CASE BD.Sub_2SC_State OF
	0: 
	        BD.V2a    := 0;
	        BD.V2b    := 1;
	1: 
	        BD.V2a    := 1;
	        BD.V2b    := 0;
	13: 
	    ;
	END_CASE;
	Step_2SC := INT_TO_WORD( BD.Sub_2SC_State );
	CASE BD.Sub_3T_State OF
	0: 
	    BD.V9 := 0;
	1: 
	    IF BD.TankLevel < 10.0 THEN
	        BD.V9 := 1;
	    END_IF;
	    IF BD.TankLevel > 80.0 THEN
	        BD.V9 := 0;
	    END_IF;
	6: 
	    reg35P_0002(
	        A    := MapARange(
	                v := BD.TankLevel,
	                i1:=BD.TankLevel_L2 - BD.TankLevel_L2_halfband,    i2:=BD.TankLevel_L2 + BD.TankLevel_L2_halfband,
	                o1:=0.0,    o2:=100.0        
	            ),
	        SP    := 50.0        
	        mode    := 1,
	        enable    := 1,
	        off_value    := 0.0,
	        kp    := 0.0,
	        k3_P1    := 0.025,
	        k3_P2    := 0.07,
	        E_P1    := 1.0,
	        E_P2    := 10.0,
	        t5    := 10,
	        k5_max    := 270,
	        CZQ_k5low    := 10,
	        CZQ_k5high    := 200,
	        CZ_t    := 10.0,
	        k13_max    := 0.2,
	        CZQ_k13low    := 10,
	        CZQ_k13high    := 200,
	        QFC_up    := 20.0,
	        QFC_down    := 20.0,
	        Q    => BD.V6_Perc
	    );
	789: 
	    reg35P_0002(
	        A    := MapARange(
	                v := BD.TankLevel,
	                i1:=60,    i2:=85,
	                o1:=0.0,    o2:=100.0        
	            ),
	        SP    := 50.0        
	        mode    := 1,
	        enable    := 1,
	        off_value    := 0.0,
	        kp    := 0.0,
	        k3_P1    := 0.025,
	        k3_P2    := 0.07,
	        E_P1    := 1.0,
	        E_P2    := 10.0,
	        t5    := 10,
	        k5_max    := 270,
	        CZQ_k5low    := 10,
	        CZQ_k5high    := 200,
	        CZ_t    := 10.0,
	        k13_max    := 0.2,
	        CZQ_k13low    := 10,
	        CZQ_k13high    := 200,
	        QFC_up    := 20.0,
	        QFC_down    := 20.0,
	        Q    => BD.V6_Perc
	    );
	END_CASE;
	Step_3T := INT_TO_WORD( BD.Sub_3T_State );
    CASE BD.MainState OF
    0: 
        BD.MainState_sub := 0;
        BD.LL_Level_420_i1 := 14050.0;
        BD.V6_Perc    := 0;
        BD.Sub_3T_State    := 0;
        BD.V4    := 0;
        BD.V7    := 0;
        BD.V3    := 0;
        BD.Sub_2SC_State    := 0;
        BD.V1    := 0;
        BD.Vhs    := 0;
        BD.Vhs_Perc    := 0;
        BD.t3_SP    := 0;
        BD.t3_heating    := 0;
        BD.Pump1    := 0;
        BD.Pump1_Perc    := 0.0;
        BD.Pump2    := 0;
        BD.Pump3    := 0;
        kt19_0001(xReset:=1, T:=T#0s);
        StatusButton1 := 1;
        StatusButton2 := 1;
        StatusButton3 := 1;
        IF ErrorMsg = 0 AND NOT ErrorPresence THEN
            IF ep_GA1_Btn.Q01 THEN
                BD.MainState := 1;
            ELSIF ep_GA2_Btn.Q01 THEN
                BD.MainState := 2;
            ELSIF ep_GA3_Btn.Q01 THEN
                BD.MainState := 13;
            END_IF;
        END_IF;
    1: 
        CASE BD.MainState_sub OF
        0: 
            BD.MainState_sub := 10;
            ErrorMsg := 3;
            ErrorPresence := 0;
        1: 
            BD.V6_Perc    := 100;
            BD.Sub_3T_State    := 0;
            BD.V4    := 1;
            BD.V7    := 0;
            BD.V3    := 0;
            BD.Sub_2SC_State    := 13;
            BD.V2a    := 0;
            BD.V2b    := 1;
            BD.V1    := 0;
            BD.Pump1    := 1;
            BD.Pump1_Perc    := 100.0;
            BD.Pump2    := 1;
            BD.t3_SP    := 60;
            BD.t3_heating    := 1;
            kt19_0001(xReset:=0, T:=T#40s);
            IF kt19_0001.xEnd THEN
                kt19_0001(xReset:=1, T:=T#0s);
                BD.MainState_sub := 2;
            END_IF;
        2: 
            BD.V2a    := 0;
            BD.V2b    := 1;
            BD.V1    := 1;
            BD.Pump2    := 0;
            kt19_0001(xReset:=0, T:=T#7s);
            IF kt19_0001.xEnd THEN
                kt19_0001(xReset:=1, T:=T#0s);
                BD.MainState_sub := 3;
            END_IF;
        3: 
            BD.V2a    := 0;
            BD.V2b    := 1;
            BD.V1    := 0;
            BD.Pump2    := 1;
            kt19_0001(xReset:=0, T:=T#10s);
            IF kt19_0001.xEnd THEN
                kt19_0001(xReset:=1, T:=T#0s);
                BD.MainState_sub := 4;
            END_IF;
        4: 
            BD.V2a    := 1;
            BD.V2b    := 0;
            BD.V1    := 0;
            BD.Pump2    := 1;
            kt19_0001(xReset:=0, T:=T#12s);
            IF kt19_0001.xEnd THEN
                kt19_0001(xReset:=1, T:=T#0s);
                BD.MainState_sub := 1;
            END_IF;
        END_CASE;
        CASE BD.MainState_sub OF
        1..4:
            IF BD.Flow < 5.0 AND BD.TankLevel < 20.0 THEN
                kt19_0003(xReset:=0, T:=T#40s);
            END_IF;
            IF kt19_0003.xEnd THEN
                kt19_0003(xReset:=1, T:=T#0s);
                BD.MainState_sub    := 10;
                ErrorMsg    := 3;
                ErrorPresence    := 0;
            END_IF;
        10:
            BD.t3_heating    := 0;
            BD.Pump1    := 0;
            BD.Pump1_Perc    := 0.0;
            BD.Pump2    := 0;
            IF BD.TankLevel > 30.0 THEN
                BD.MainState_sub    := 1;
                ErrorMsg    := 0;
                ErrorPresence    := 0;
            END_IF;
        END_CASE;
        CASE BD.MainState_sub OF
        0..254:
            StatusButton1 := 2;
            StatusButton2 := 0;
            StatusButton3 := 0;
            IF ep_GA1_Btn.Q01 THEN
                BD.MainState := 0;
                BD.MainState_sub := 255;
            END_IF;
        END_CASE;
        IF BD.t3_real > BD.t3_SP * 1.1 THEN
            BD.t3_heating    := 0;
        END_IF;
        IF BD.MainState_sub = 255 THEN
            BD.MainState_sub := 0;
        END_IF;
    2: 
        StatusButton1 := 0;
        StatusButton2 := 0;
        StatusButton3 := 0;
        CASE BD.MainState_sub OF
        0: 
            BD.MainState_sub := 1;
        1:
            BD.V6_Perc    := 0.0;
            BD.Sub_3T_State    := 1;
            BD.V4    := 0;
            BD.V7    := 1;
            BD.V3    := 0;
            BD.V1    := 0;
            BD.Pump1    := 1;
            BD.Pump1_Perc    := 100.0;
            BD.Pump2    := 1;
            kt19_0001(xReset:=0, T:=T#40s);
            IF kt19_0001.xEnd THEN
                kt19_0001(xReset:=1, T:=T#0s);
                BD.MainState_sub := 2;
            END_IF;
        2:
            BD.Sub_3T_State    := 0;
            ErrorMsg    := 20;
            ErrorPresence    := 0;
            IF BD.Flow < 3.0 THEN
                BD.MainState_sub := 3;
            END_IF;
        3:
            BD.Pump1    := 1;
            BD.Pump1_Perc    := 70.0;
            BD.Pump2    := 0;
            kt19_0001(xReset:=0, T:=T#15s);
            IF kt19_0001.xEnd THEN
                kt19_0001(xReset:=1, T:=T#0s);
                BD.MainState_sub := 4;
            END_IF;
        4:
            BD.LL_Level_420_i1    := avg01_0005.Q;
            ErrorMsg    := 0;
            ErrorPresence    := 0;
            BD.MainState_sub    := 5;
        5:
            BD.V6_Perc    := 0.0;
            BD.Sub_3T_State    := 1;
            BD.V4    := 0;
            BD.V7    := 1;
            BD.V3    := 0;
            BD.V1    := 0;
            BD.Pump1    := 1;
            BD.Pump1_Perc    := 100.0;
            BD.Pump2    := 1;
            kt19_0001(xReset:=0, T:=T#250s);
            IF kt19_0001.xEnd THEN
                kt19_0001(xReset:=1, T:=T#0s);
                BD.MainState_sub := 255;
                BD.MainState := 3;
            END_IF;
        END_CASE;
        CASE BD.MainState_sub OF
        1..4:
            IF dtclk.Q THEN
                avg01_0005(
                    init    := 0,
                    X    := INT_TO_REAL(LL_Level_420),
                    t    := 4,
                );
            END_IF;
        END_CASE;
        IF BD.MainState_sub = 255 THEN
            BD.MainState_sub := 0;
        END_IF;
    3: 
        BD.V6_Perc    := 0.0;
        BD.V4    := 0;
        BD.V7    := 0;
        BD.V3    := 1;
        BD.t3_SP    := 95;
        BD.t3_heating    := 1;
        IF BD.t4_real > 85 THEN
            BD.MainState := 4;
        END_IF;
        StatusButton1 := 0;
        StatusButton2 := 0;
        StatusButton3 := 0;
    4: 
        BD.V6_Perc    := 0.0;
        BD.V4    := 0;
        BD.V7    := 0;
        BD.V3    := 1;
        BD.t3_SP    := 95;
        BD.t3_heating    := 1;
