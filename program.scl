FUNCTION_BLOCK "EdgePulse_code"
    VAR_INPUT
        CLK:    BOOL;
    END_VAR
    VAR_OUTPUT
        Q0:    BOOL;
        Q1:    BOOL;
        Q01:    BOOL;
    END_VAR
    VAR
        CLK_prev:    BOOL;
    END_VAR
    IF #CLK = #CLK_prev THEN
        #Q0:=0;    #Q1:=0;    #Q01:=0;
    ELSE
        IF #CLK THEN #Q1:=1; ELSE #Q0:=1; END_IF;
        #Q01 := 1;
        #CLK_prev := #CLK;
    END_IF;
END_FUNCTION_BLOCK
